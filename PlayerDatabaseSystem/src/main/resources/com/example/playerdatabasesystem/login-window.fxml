<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/22.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.playerdatabasesystem.LoginWindow">
   <children>
      <Text layoutX="83.0" layoutY="96.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Club Name:" />
      <TextField fx:id="username" layoutX="158.0" layoutY="78.0" onKeyTyped="#usernameToTitleCase" />
      <Text layoutX="90.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <PasswordField fx:id="password" layoutX="158.0" layoutY="124.0" />
      <Button layoutX="164.0" layoutY="190.0" mnemonicParsing="false" onAction="#onLoginButtonClicked" text="Login" />
      <Button layoutX="248.0" layoutY="191.0" mnemonicParsing="false" onAction="#onRegisterButtonClicked" text="Register" />
   </children>
</AnchorPane>
